import java.util.Scanner;

public class TusTakimi {
	   public String veriAl()
	    {
	    	String deger;
	        Scanner scan=new Scanner(System.in);
	        deger=scan.next(); scan.nextLine();
	        return deger;
	    }
	    
	    public int degerAl()
	    {
	    	Scanner scan=new Scanner(System.in);
	    	return scan.nextInt();
	    }
}
