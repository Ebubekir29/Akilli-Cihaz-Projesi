	IEkran ekran;
	
 public void kullaniciOlustur()
 {
	 ekran.mesajGoruntule("Kullanici Adinizi Giriniz : \n ");
     String ad = scan.next();
     ekran.mesajGoruntule("Sifrenizi Giriniz : ");
     String sifre = scan.next();
 }
}
